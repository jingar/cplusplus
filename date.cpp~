#include <iostream>
#include <stdexcept>

struct Date
{
  int y;
  int m;
  int d;
};
void intialize_date(Date& date,int y,int m, int d)
{
  if(m <=0 || m > 12)
    {
      throw runtime_error("month must be between 1 and 12");
    }
  if(d > 31)
    {
      throw runtime_error("day must be less than or equal to 31");
    }
  if(y < 0)
    {
      throw runtime_error("year must be greater than 0");
    }
}

