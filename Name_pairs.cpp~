#include <iostream>
#include <vector>
#include <string>
#include <sstream>

class Name_pairs
{
  
public:
  void read_names();
  void read_ages();
  void print();
  void sort();
  const std::vector<std::string>& getNames();
  const std::vector<double>& getAges();
private:
  std::vector<std::string> names;
  std::vector<double> ages; 
};


const std::vector<std::string>& Name_pairs::getNames()
{
  return names;
}

void Name_pairs::read_names()
{
  std::vector<std::string> v = getNames();
  std::string name;
  std::string line;
  std::getline(std::cin,line);
  std::istringstream iss(line);
  while(iss >> name)
    {
      v.push_back(name);
    }
}

void Name_pairs::read_ages()
{
  std::vector<std::string> v = getNames();
  int age;
  std::string line;
  std::getline(std::cin,line);
  std::istringstream iss(line);
  while(iss >> age)
    {
      v.push_back(age);
    }
}


int main()
{

}
